def main() {
  var x = 0 : int;
  var y = 0 : int;
  var z = 0 : int;

  x = 42;
  y = 21;
  z = 5;
  print(x + y);   // 63
  print(x - y);   // 21
  print(x * y);   // 882
  print(x / y);   // 2
  print(x / z);   // 8
  print(x % z);   // 2
  print(-x);      // -42
  print(-x / z);  // -8
  print(-x % z);  // -2
}